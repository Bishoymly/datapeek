{
  "name": "datapeek",
  "version": "0.1.2",
  "description": "A local SQL database browser CLI tool",
  "type": "module",
  "bin": {
    "datapeek": "./dist/cli/index.js"
  },
  "exports": {
    ".": "./dist/server/index.js",
    "./cli": "./dist/cli/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./dist/server/index.js",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "concurrently -n server,client -c blue,green \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsup src/server/dev.ts && concurrently --kill-others \"tsup src/server/dev.ts --watch\" \"nodemon dist/server/dev.js\"",
    "dev:client": "vite --open",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsup",
    "prepublishOnly": "npm run build",
    "postinstall": "chmod +x dist/cli/index.js || true"
  },
  "keywords": [
    "sql",
    "database",
    "browser",
    "cli",
    "mssql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "mssql": "^11.0.1",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-table": "^8.20.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "lucide-react": "^0.468.0",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.5"
  }
}
